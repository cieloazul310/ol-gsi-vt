---
import Layout from "../layout/layout.astro";
import { menu, siteMetadata } from "../data";

interface Props {
  frontmatter?: {
    title?: string;
    description?: string;
  };
}

const { frontmatter } = Astro.props;
---

<Layout
  siteMetadata={siteMetadata}
  menu={menu}
  title={frontmatter?.title}
  description={frontmatter?.description}
>
  <div>
    <slot />
  </div>
</Layout>
